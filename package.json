  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc --noEmit --pretty",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "commit": "cz",
    "email:dev": "email dev --dir src/emails",
    "prisma:update": "npx prisma generate && npx prisma db push",
    "prisma:clean": "npx prisma generate && npx prisma migrate reset && npx prisma db push",
    "prisma:generate": "npx prisma generate",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "serve-storybook": "run-s storybook:*",
    "test-storybook:ci": "start-server-and-test serve-storybook http://127.0.0.1:6006 test-storybook",
    "prepare": "husky",
    "serve": "json-server --watch db.json --port 8000",
    "server": "json-server --watch db1.json --port 8001"
  },
